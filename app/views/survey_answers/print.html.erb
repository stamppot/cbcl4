<%= stylesheet_link_tag('survey_print') %>

<div id="survey_header">
	<div class="survey span-24" style='background-color: #fff<%# @survey.color %>;'>
		<span id="survey_journal_info" class="span-6">
			ID: <%= @journal_entry.journal.qualified_id %><br>
			Navn: <%= @journal_entry.journal.name %><br>
			F&oslash;dselsdato: <%= @journal_entry.journal.birth_short %>
		</span>
		<span class="surveyname span-18"><%= @survey.get_title %></span>
	</div>
</div>
<br/>

<br/>

<div style="text-align: left;" class="span-24">
	&nbsp;
	<%= render :partial => "surveys/answer_by" %></div>
<br/>
<%= render :partial => "question", :collection => @survey.questions %>

<p/>